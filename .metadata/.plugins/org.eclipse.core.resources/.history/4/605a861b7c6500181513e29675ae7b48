package com.cn.threadTest;

public class threadTest {

	public static void main(String[] args) {
		
		MyThread thread  = new MyThread();
		
		thread.run();
		
		for (int i = 0; i < 1000; i++) {
			
			System.out.println("main main main");
		}
	}
}

class MyThread extends Thread {
	
	public void run(){
		 
		for (int i = 0; i < 1000; i++) {
			
			System.out.println("*******");
		}
	}
}
